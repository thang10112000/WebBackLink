@model List<Model.EF.About>
@{
    ViewBag.Title = "Tổng quan về ClickShop";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="test">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 ">
            </div>
        </div>

    </div>
</div>
<div class="ps-product--detail pt-60">
    <div class="ps-container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-10 col-md-12 col-lg-offset-1">
                    <div class="ps-product__thumbnail">
                        <div class="ps-product__preview">

                        </div>
                        <div class="ps-product__image">
                            <div class="item"><img src="/Assets/client/images/partner.jpg" alt=""></div>

                        </div>
                    </div>
                    <div class="ps-product__thumbnail--mobile">
                        <div class="ps-product__main-img"><img src="/Assets/client/images/partner.jpg" alt=""></div>
                      
                    </div>
                    <div class="ps-product__info">

                        <h1>@item.Name</h1>


                        <div class="ps-product__block ps-product__quickview">
                            <h4>REVIEW</h4>
                            <h4>@item.Description</h4>
                        </div>

                    </div>
                    <div class="clearfix"></div>

                    <div class="tab-content mb-60">
                        <div class="tab-pane active" role="tabpanel" id="tab_01">
                            <h4>
                                @Html.Raw(item.Detail)
                            </h4>
                        </div>
                    </div>
                </div>

            }
        </div>
    </div>
</div>

