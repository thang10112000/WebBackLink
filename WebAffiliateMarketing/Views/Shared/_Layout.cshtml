<body class="ps-loading">
    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v12.0&appId=969741493638776&autoLogAppEvents=1" nonce="eo42BPz9"></script>
    <div class="header--sidebar"></div>
    <header class="header">
        <dxiv class="header__top">
            @{ Html.RenderAction("TopMenu", "Home");}
        </dxiv>
    
        <nav class="navigation">
            <div class="container-fluid">
                <center>
                    <div class="navigation__column left">
                        <div class="header__logo"><a class="ps-logo" href="/" ref="nofollow"><img src="~/Assets/admin/images/clickshoplogo1.png" alt=""></a></div>
                    </div>
                </center>
                @{Html.RenderAction("MainMenu", "Home"); }

                <div class="navigation__column right">
                    <form class="ps-search--header" action="/tim-kiem" method="get">
                        <input class="form-control" id="txtKeyword" name="keyword" type="text" placeholder="Tìm kiếm…">
                        <button><i class="ps-icon-search"></i></button>
                    </form>

                    @*giỏ hàng*@
                    @{ Html.RenderAction("HeaderCart", "Home");}
                    <div class="menu-toggle"><span></span></div>
                </div>
            </div>
        </nav>
    </header>
    <div class="header-services">
        <div class="ps-services owl-slider" data-owl-auto="true" data-owl-loop="true" data-owl-speed="7000" data-owl-gap="0" data-owl-nav="true" data-owl-dots="false" data-owl-item="1" data-owl-item-xs="1" data-owl-item-sm="1" data-owl-item-md="1" data-owl-item-lg="1" data-owl-duration="1000" data-owl-mousedrag="on">
            <p class="ps-service"><i class="ps-icon-delivery"></i><strong>Xin chào</strong>: Shop pro click là mua</p>
            <p class="ps-service"><i class="ps-icon-delivery"></i><strong>Xin chào</strong>: Shop pro click là mua</p>

        </div>
    </div>

    <main class="ps-main">

        @RenderBody()



        <div class="footer">
            <div>
                @RenderSection("slide", required: false)
            </div>
            @{ Html.RenderAction("Footer", "Home");}
        </div>
    </main>


